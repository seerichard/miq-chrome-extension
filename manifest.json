{
  "manifest_version": 2,
  "name": "MIQ",
  "description": "Just want to get home",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}